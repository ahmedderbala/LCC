@{
    ViewData["Title"] = "Predictions Archive List";
    Layout = "~/Views/Shared/adminlayout.cshtml"; 
}
@using webproject2.Model;
@model List<Prediction>;
@section content{
   


                <h1 class="page-heading h2">Predictions Archive List</h1>

                <div class="card">
                    <div class="card-header">
                        @*<h4 class="card-title">Questions</h4>*@
                    </div>
                    <div class="card-header bg-white">
                        <a href="/nn/predict" class="btn btn-success">Create Prediction <i class="material-icons">add</i></a>           
                    </div>
                    <div class="nestable" id="nestable">

                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <td>#</td>
                                    <td>Project Name</td>
                                    <td>Prediction Date</td>
                                    <td> Actions</td>

                                </tr>
                            </thead>
                            <tbody>
                                 @foreach (var item in Model)
                                {
                                <tr>
                                   <td>@item.Id</td>
                                    <td>@item.Projectname</td>
                                    <td>@item.Createdat</td>
                                    <td> 
                                       <a  href="/nn/predict/@item.Id" class="btn btn-primary btn-sm" style="color:#fff !important;">View</a>
                                       <a class="btn btn-danger btn-sm delete"
                                                   data-id="@item.Id"
                                                   data-controller="nn"
                                                   data-action="DeletePrediction"
                                                   data-body-message="Dou yo sure that you want to delete this record">Delete</a>

                                    </td>
                                </tr>
                                }                               
                            </tbody>
                        </table>

                    </div>
                </div>
                <nav class="text-center">
                    <ul class="pagination pagination-sm">




                       


                    </ul>
                </nav>
            </div>
        </div>

        <div class="modal fade" id="editQuiz">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header bg-primary">
                        <h5 class="modal-title">Add/Edit Question</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <form action="#">
                            <div class="form-group row">
                                <label for="qtitle" class="col-form-label col-md-3">Title:</label>
                                <div class="col-md-9">
                                    <input id="qtitle" type="text" class="form-control" value="Database Access">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="type" class="col-form-label col-md-3">Type:</label>
                                <div class="col-md-4">
                                    <select id="type" class="custom-control custom-select form-control">
                                        <option value="1">Input</option>
                                        <option value="2">Textarea</option>
                                        <option value="3">Checkbox</option>
                                        <option value="3">Radio</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-form-label col-md-3">Answers:</label>
                                <div class="col-md-9">
                                    <a href="#" class="btn btn-default"><i class="material-icons">add</i> Add Answer</a>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="touch-spin-2" class="col-form-label col-md-3">Question Score:</label>
                                <div class="col-md-4">
                                    <input id="touch-spin-2" data-toggle="touch-spin" data-min="0" data-max="100" data-step="5" type="text" value="50" name="demo2" class="form-control" />
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-8 offset-md-3">
                                    <button type="submit" class="btn btn-success">Save</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
           
}